{
  "name": "searchbeam",
  "version": "0.1.0",
  "description": "高性能云端视频搜索API服务，代理多平台搜索请求",
  "main": "dist/app.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "search",
    "api",
    "youtube",
    "video",
    "fastify",
    "nodejs"
  ],
  "author": "Samzong",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "8.3.0",
    "@fastify/env": "4.2.0",
    "axios": "1.4.0",
    "dotenv": "16.3.1",
    "fastify": "4.20.0",
    "lru-cache": "10.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint": "8.45.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.1.6"
  }
} 