# YouTube API Keys,multiple tokens separated by commas (required)
# Create at https://console.developers.google.com/
YOUTUBE_API_KEYS=your_youtube_api_key

# Authentication token list, multiple tokens separated by commas (required)
AUTH_TOKENS=token1,token2,token3

# Cache Configuration
# Maximum number of cached entries
CACHE_MAX_ITEMS=1000
# Cache expiration time (ms), 10 minutes by default
CACHE_TTL=600000

# Server Configuration
PORT=3000
# Allowed cross-domain name, multiple domain names are separated by commas, set to * to allow all domain names
CORS_ORIGIN=*

# Paging Configuration
DEFAULT_PAGE_SIZE=20
MAX_PAGE_SIZE=50
